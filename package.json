{
  "name": "react-router-web",
  "version": "1.0.0",
  "description": "react-router",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --updateSnapshot",
    "start": "webpack-dev-server --history-api-fallback --progress --colors",
    "build": "webpack --progress --colors --config webpack.prod.js",
    "extstart": "webpack-dev-server --config webpack.ext.js --history-api-fallback --progress --colors",
    "mockstart": "webpack-dev-server --config webpack.mock.js --history-api-fallback --progress --colors",
    "dllstart": "webpack-dev-server --config webpack.dll.js --history-api-fallback --progress --colors",
    "dllconfig": "webpack --config webpack.dll.config.js --progress --colors"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "bundle-loader": "^0.5.5",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-to-json": "^3.3.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.4.3",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "mockjs": "^1.0.1-beta3",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "^2.0.8",
    "react-test-renderer": "^15.6.2",
    "style-loader": "^0.19.0",
    "sugarss": "^1.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "antd": "^3.2.3",
    "baobab": "^2.5.0",
    "baobab-react": "^3.0.0",
    "file-saver": "^1.3.3",
    "history": "^4.7.2",
    "jsencrypt": "2.3.1",
    "moment": "^2.20.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1",
    "reqwest": "^2.0.5"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "testRegex": ".*\\.test\\.js$",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "src/**/**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/spec/fileMock.js",
      "\\.(css|scss|less)$": "<rootDir>/spec/styleMock.js",
      "utils/common$": "<rootDir>/utils/common/index.js",
      "components(.*)$": "<rootDir>/components/$1"
    },
    "setupFiles": [
      "<rootDir>/spec/setup.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}